<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img6"
    tools:context=".RegisterMovie">


    <TextView
        android:id="@+id/title"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="173dp"
        android:background="#020101"
        android:text="@string/title_of_movie"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editOne"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="false"
        android:layout_marginStart="172dp"
        android:layout_marginBottom="519dp"
        android:gravity="center"
        android:hint="Enter Movie Title"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <TextView
        android:id="@+id/year"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="266dp"
        android:background="#000000"
        android:text="Year of Release: "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editTwo"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="171dp"
        android:layout_marginBottom="424dp"
        android:gravity="center"
        android:hint="Enter Year"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <TextView
        android:id="@+id/director"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="345dp"
        android:background="#020101"
        android:text="Director of Movie: "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editThree"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="172dp"
        android:layout_marginBottom="347dp"
        android:gravity="center"
        android:hint="Enter Director Name"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <TextView
        android:id="@+id/cast"
        android:layout_width="115dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="414dp"
        android:background="#0B0808"
        android:text="Cast: "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editFour"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="170dp"
        android:layout_marginBottom="281dp"
        android:gravity="center"
        android:hint="Enter Cast Name"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <TextView
        android:id="@+id/rating"
        android:layout_width="115dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="498dp"
        android:background="#090707"
        android:text="Ratings: "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editFive"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="170dp"
        android:layout_marginBottom="197dp"
        android:gravity="center"
        android:hint="Enter Rating"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <TextView
        android:id="@+id/review"
        android:layout_width="115dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="578dp"
        android:background="#120F0F"
        android:text="Review: "
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <EditText
        android:id="@+id/editSix"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="169dp"
        android:layout_marginBottom="118dp"
        android:gravity="center"
        android:hint="Enter Review"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />


    <Button
        android:id="@+id/save"
        android:layout_width="408dp"
        android:layout_height="63dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="7dp"
        android:elevation="5dp"
        android:text="Save"
        app:backgroundTint="#130707"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:targetApi="lollipop" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="336dp"
        android:layout_marginBottom="682dp"
        android:background="?attr/actionModeCloseDrawable"
        android:elevation="5dp"
        android:onClick="getToHomePage"
        app:backgroundTint="#130707"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:targetApi="lollipop" />
</RelativeLayout>

