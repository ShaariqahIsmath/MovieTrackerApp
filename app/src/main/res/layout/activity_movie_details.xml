<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img10"
    tools:context=".DisplayMovies">

    <EditText
        android:id="@+id/edOne"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="false"
        android:layout_marginStart="98dp"
        android:layout_marginBottom="518dp"
        android:gravity="center"
        android:hint="Enter Movie Title"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />


    <EditText
        android:id="@+id/edTwo"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="432dp"
        android:gravity="center"
        android:hint="Enter Year"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />


    <EditText
        android:id="@+id/edThree"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="353dp"
        android:gravity="center"
        android:hint="Enter Director Name"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <EditText
        android:id="@+id/edFour"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="275dp"
        android:gravity="center"
        android:hint="Enter Cast Name"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />


    <RatingBar
        android:id="@+id/edFive"
        style="@style/Widget.AppCompat.RatingBar.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="136dp"
        android:layout_marginBottom="192dp"
        android:gravity="center"
        android:isIndicator="false"
        android:numStars="10"
        android:progressBackgroundTint="@color/black"
        android:progressTint="@color/yellow"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <EditText
        android:id="@+id/edSix"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="114dp"
        android:gravity="center"
        android:hint="Enter Review"
        android:textColorHint="@color/material_on_background_emphasis_high_type" />

    <Button
        android:id="@+id/saved"
        android:layout_width="353dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="26dp"
        android:layout_marginBottom="20dp"
        android:text="Save"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/returnToEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="294dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="13dp"
        android:layout_marginBottom="651dp"
        android:text="Return to Display"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/homeButton"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="340dp"
        android:layout_marginBottom="661dp"
        android:background="?attr/actionModeCloseDrawable"
        android:elevation="5dp"
        android:onClick="getToHomePage"
        app:backgroundTint="#130707"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:targetApi="lollipop" />




</RelativeLayout>